{
  "name": "journey",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint ./*.js ./js/ ./__test__/",
    "fix": "eslint ./*.js ./js/ ./__test__/ --fix",
    "start": "node node_modules/react-native/local-cli/cli.js start",
