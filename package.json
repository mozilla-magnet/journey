    "react-native-heading": "git://github.com/arcturus/react-native-heading.git",
    "react-native-maps": "^0.13.0",
    "react-redux": "^5.0.1",
    "realm": "^0.15.4",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.13.1",
    "eslint-plugin-react": "^6.9.0",
    "jest": "18.1.0",
    "react-test-renderer": "~15.4.0-rc.4"
